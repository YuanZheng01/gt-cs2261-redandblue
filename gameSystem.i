    }

    if (frame == (redandblueR[gameRcount] - (line.row / velocity))) {
        generateRed();
        gameRcount++;
    }
}



void drawScore(int x, int y) {
    int temp = score;
    for (int i = 0; i < 8 && temp != 0; i++) {
        shadowOAM[shadowCount].attr0 = (0xFF & y) | (0 << 14);
        shadowOAM[shadowCount].attr1 = (0x1FF & (x - (1 + i) * 16)) | (1 << 14);
        shadowOAM[shadowCount++].attr2 = ((0) << 12) | ((1 * 2)*32 + ((temp % 10) * 2));
        temp /= 10;
    }
}

void drawCombo() {
    int temp = combo;
    int len = 0;
    int xoff = 0;
    while (temp > 0) {
        len++;
        temp /= 10;
    }
    temp = combo;

    switch(len) {
        case 1:
            xoff = -8;
            break;
        case 2:
            xoff = 0;
            break;
        case 3:
            xoff = 8;
            break;
        case 4:
            xoff = 16;
            break;
    }

    for (int i = 0; i < len && temp != 0; i++) {
        shadowOAM[shadowCount].attr0 = (0xFF & 100) | (0 << 14);
        shadowOAM[shadowCount].attr1 = (0x1FF & (240 / 2 + xoff - i * 16)) | (1 << 14);
        shadowOAM[shadowCount++].attr2 = ((0) << 12) | ((1 * 2)*32 + ((temp % 10) * 2));
        temp /= 10;
    }
}

void drawFrame() {
    int temp = frame;
    for (int i = 0; i < 6 && temp != 0; i++) {
        shadowOAM[shadowCount].attr0 = (0xFF & 16) | (0 << 14);
        shadowOAM[shadowCount].attr1 = (0x1FF & (240 - (1 + i) * 16)) | (1 << 14);
        shadowOAM[shadowCount++].attr2 = ((0) << 12) | ((1 * 2)*32 + ((temp % 10) * 2));
        temp /= 10;
    }
}
