            && blue->row + blue->yCalibrate < line.row + line.yCalibrate + 60
            && blue->active) {


            if (blue->row + blue->yCalibrate > line.row + line.yCalibrate - 10
                && blue->row + blue->yCalibrate < line.row + line.yCalibrate + 5) {
                score += 100;
                perfectCount++;
                combo++;

                recycleNote(blue);
                blue = &blues[(blue->id + 1) % 9];

            } else if (blue->row + blue->yCalibrate > line.row + line.yCalibrate - 25
                && blue->row + blue->yCalibrate < line.row + line.yCalibrate + 15) {
                score += 80;
                greatCount++;
                combo++;

                recycleNote(blue);
                blue = &blues[(blue->id + 1) % 9];

            } else {
                score += 10;
                badCount++;
                combo = 0;

                playSoundB(hitsound_data, hitsound_length, 0);

                recycleNote(blue);
                blue = &blues[(blue->id + 1) % 9];
            }
        }
}
# 244 "gameSystem.c"
void recycleNote(NOTE* note) {
    note->active = 0;
    note->col = 0;
    note->row = 0;
    note->height = 4;
    note->width = 32;
    note->yCalibrate = 8;
    note->currFrame = 0;
}

void initNotes() {
    for (int i = 0; i < 9; i++) {

        reds[i].id = i;
        reds[i].active = 0;
        reds[i].col = 0;
        reds[i].row = 0;
        reds[i].height = 4;
        reds[i].width = 32;
        reds[i].yCalibrate = 8;
        reds[i].velocity = velocity;
        reds[i].currFrame = 0;



        blues[i].id = i;
        blues[i].active = 0;
        blues[i].col = 0;
        blues[i].row = 0;
        blues[i].height = 4;
        blues[i].width = 32;
        blues[i].yCalibrate = 8;
        blues[i].velocity = velocity;
        blues[i].currFrame = 0;
    }
}

void generateRed() {


    for (int i = 0; i < 9; i++) {
        if (!reds[(i + red->id) % 9].active) {
            reds[(i + red->id) % 9].col = (240 / 3) - (reds[i % 9].width / 2);
            reds[(i + red->id) % 9].row = 0;
            reds[(i + red->id) % 9].active = 1;
            return;
        }
    }
}

void generateBlue() {


    for (int i = 0; i < 9; i++) {
        if (!blues[(i + blue->id) % 9].active) {
            blues[(i + blue->id) % 9].col = (2 * 240 / 3) - (blues[i % 9].width / 2);
            blues[(i + blue->id) % 9].row = 0;
            blues[(i + blue->id) % 9].active = 1;
            return;
        }
    }
}


void updateNotes() {

    if (red->row >= line.row + line.yCalibrate + 30) {
        recycleNote(red);
        playSoundB(hitsound_data, hitsound_length, 0);
        ((unsigned short *)0x5000000)[1] = ((unsigned short *)0x5000000)[4];
        red = &reds[(red->id + 1) % 9];
        combo = 0;
    }

    if (blue->row >= line.row + line.yCalibrate + 30) {
        recycleNote(blue);
        playSoundB(hitsound_data, hitsound_length, 0);
        blue = &blues[(blue->id + 1) % 9];
        ((unsigned short *)0x5000000)[1] = ((unsigned short *)0x5000000)[5];
        combo = 0;
    }


    for (int i = 0; i < 9; i++) {
        if (cheater == 9 && reds[i].row + reds[i].yCalibrate > line.row + line.yCalibrate - 5
                && reds[i].row + reds[i].yCalibrate < line.row + line.yCalibrate + 5) {
                    hitRed();
                    playSoundB(hitsound_data, hitsound_length, 0);
                    ((unsigned short *)0x5000000)[2] = ((unsigned short *)0x5000000)[4];
        }

        if (cheater == 9 && blues[i].row + blues[i].yCalibrate > line.row + line.yCalibrate - 5
                && blues[i].row + blues[i].yCalibrate < line.row + line.yCalibrate + 5) {
                    hitBlue();
                    playSoundB(hitsound_data, hitsound_length, 0);
                    ((unsigned short *)0x5000000)[3] = ((unsigned short *)0x5000000)[5];
        }

        if (reds[i].active) {
            reds[i].row += reds[i].velocity;
        }
        if (blues[i].active) {
            blues[i].row += blues[i].velocity;
        }
    }
}



void drawNotes() {
    for (int i = 0; i < 9; i++) {
        if (reds[i].active) {
            shadowOAM[shadowCount].attr0 = reds[i].row | (0 << 13) | (1 << 14);
            shadowOAM[shadowCount].attr1 = reds[i].col | (1 << 14);
            shadowOAM[shadowCount++].attr2 = ((0) << 12) | ((4)*32 + (reds[i].currFrame * 4));
            reds[i].currFrame = (reds[i].currFrame + 1) % 3;
        } else {
            shadowOAM[shadowCount++].attr0 = (2 << 8);
        }
        if (blues[i].active) {
            shadowOAM[shadowCount].attr0 = blues[i].row | (0 << 13) | (1 << 14);
            shadowOAM[shadowCount].attr1 = blues[i].col | (1 << 14);
            shadowOAM[shadowCount++].attr2 = ((0) << 12) | ((5)*32 + (blues[i].currFrame * 4));
            blues[i].currFrame = (blues[i].currFrame + 1) % 3;
        } else {
            shadowOAM[shadowCount++].attr0 = (2 << 8);
        }
    }
}


void noteGenerator() {

    if (frame == (redandblueB[gameBcount] - (line.row / velocity))) {
        generateBlue();
